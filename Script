----------------------------------------------------------------------
-- 7. Utility Functions
----------------------------------------------------------------------

-- Function to create a button for a glove/script
local function createButton(buttonConfig)
    local button = Instance.new("TextButton")
    button.Name = buttonConfig.Name
    button.Size = UDim2.new(0.9, 0, 0, 50) -- 90% width, 50px height
    button.Text = buttonConfig.Text
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.BackgroundColor3 = BUTTON_COLOR
    button.BorderSizePixel = 0
    button.Font = Enum.Font.SourceSans
    button.TextSize = 18
    button.TextXAlignment = Enum.TextXAlignment.Left
    button.AutoButtonColor = false

    local UIPaddingButton = Instance.new("UIPadding")
    UIPaddingButton.PaddingLeft = UDim.new(0, 15)
    UIPaddingButton.Parent = button

    local UICornerButton = Instance.new("UICorner")
    UICornerButton.CornerRadius = UDim.new(0, 5)
    UICornerButton.Parent = button

    -- Button Action
    button.MouseButton1Click:Connect(function()
        buttonConfig.Func() -- Execute the associated function
    end)

    -- Hover effect
    button.MouseEnter:Connect(function()
        local hoverTween = TweenService:Create(button, TWEEN_INFO, {BackgroundColor3 = HOVER_COLOR})
        hoverTween:Play()
    end)
    button.MouseLeave:Connect(function()
        local leaveTween = TweenService:Create(button, TWEEN_INFO, {BackgroundColor3 = BUTTON_COLOR})
        leaveTween:Play()
    end)

    return button
end

-- Function to build the UI (simplified)
local function buildUI()
    -- Create ALL buttons and parent them directly to the main ContentFrame
    for _, config in ipairs(ALL_BUTTONS_CONFIG) do
        local button = createButton(config)
        button.Parent = ContentFrame
        table.insert(ALL_UI_BUTTONS, button)
    end
end

-- Function to perform the search and update the content frame
local function updateSearch(searchText)
    local query = searchText:lower():gsub("[^%a%d%s]", ""):gsub("%s+", "") -- Clean and normalize search text
    
    -- The search is active if the query is not empty
    IS_SEARCHING = (query ~= "")

    if IS_SEARCHING then
        for _, button in pairs(ALL_UI_BUTTONS) do
            local buttonText = button.Text:lower():gsub("[^%a%d%s]", ""):gsub("%s+", "")
            
            -- Check if the button text contains the search query
            if buttonText:find(query, 1, true) then
                button.Visible = true
            else
                button.Visible = false
            end
        end
    else
        -- If search is empty, show all buttons
        for _, button in pairs(ALL_UI_BUTTONS) do
            button.Visible = true
        end
    end

    -- Force a layout refresh (trick to update CanvasSize immediately)
    ContentListLayout.Parent = nil
    ContentListLayout.Parent = ContentFrame
end


----------------------------------------------------------------------
-- 8. Connect Events
----------------------------------------------------------------------

-- Close Button
CloseButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = false
end)

-- Toggle Button
ToggleButton.MouseButton1Click:Connect(function()
    MainFrame.Visible = not MainFrame.Visible
end)

-- Search Bar Text Change
SearchBar.Changed:Connect(function(property)
    if property == "Text" then
        updateSearch(SearchBar.Text)
    end
end)

-- Initial UI Setup
buildUI()
-- Run an initial search to ensure all buttons are displayed correctly when the panel opens
updateSearch("")
